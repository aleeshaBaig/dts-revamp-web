<div class="row p-custom">
    <div class="col">
        <h6 class="card-title p-custom1" style="font-weight: 700; font-size: 15px;padding: 10px 0 0 0;">Current Address</h6>
    </div>
</div>    

<div class="row p-custom">
    <div class="col">
        <div class="form-group">
            <label>Address</label>
            <input type="text" class="form-control" value="<%= form.address %>" readonly>
        </div>
    </div>

    <div class="col">
        <div class="form-group">
            <label>District</label>
            <input type="text" class="form-control" value="<%= form.district%>" readonly>
        </div>
    </div>

    <div class="col">
        <div class="form-group">
            <label>Town / Tehsil</label>
            <input type="text" class="form-control" value="<%= form.tehsil %>" readonly>
        </div>
    </div>
    <div class="col">
        <div class="form-group">
            <label>UC</label>
            <input type="text" class="form-control" value="<%= form.uc %>" readonly>
        </div>
    </div>

</div>


