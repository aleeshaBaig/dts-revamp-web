<%= render partial: '/users/roles/default', locals: {f: f} %>
<div class="col-md-3">
  <div class="form-group">
    <%= f.input :district_id, label: '* District', collection: districts_list, label_method: :district_name, prompt: "-Select-", input_html:{class: "form-control tehsil_user_district"}%>
  </div>
</div>

<div class="col-md-3">
  <div class="form-group">
    <%= f.input :tehsil_id, label: '* Tehsil', collection: tehsil_information(@user.district_id), prompt: "-Select-", input_html:{class: "form-control user_tehsil_list"}%>
  </div>
</div>